{
  "name": "STRK Price Monitoring",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Monitor the Starknet token price (STRK) in real-time.",
  "homepage_url": "https://github.com/remybar/XXX",
  "permissions": ["alarms", "storage"],
  "icons": {
    "128": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "STRK Price Monitoring",
    "default_popup": "popup.html"
  }
}
